{
  "compilerOptions": {
    "module": "commonjs",
    "moduleResolution": "node",
    "target": "es5",
    "sourceMap": true,
    "jsx": "react",
    "outDir": "./docs",
    "esModuleInterop": true,
    "paths": {
      "components": ["./src/components"],
      "pages": ["./src/pages"],
      "styles/*": ["./src/resources/styles/*"],
      "store/*": ["./src/store/*"],
      "image/*": ["./src/resources/image/*"],
    }
  },
  "include": [
    "./src",
  ],
  "exclude": [
    "node_modules"
  ],
  "baseUrl": "./src"
}